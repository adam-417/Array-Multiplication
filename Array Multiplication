using System;

class Program
{
    static void Main()
    {
        int num = 7;
        int length = 6;
        int[] results = new int[length];

        for (int i = 0; i < length; i++)
        {
            results[i] = (i + 1) * num;
        }

        Console.WriteLine($"Multiplication Table for {num}:");

        for (int i = 0; i < length; i++)
        {
            Console.WriteLine($"{num} x {i + 1} = {results[i]}");
        }
    }
}
